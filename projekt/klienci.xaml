<Window x:Class="projekt.klienci"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projekt"
        mc:Ignorable="d"
        Background="#939499"
        WindowStartupLocation="CenterScreen"
        Title="Klienci" Height="450" Width="800" ResizeMode="NoResize">
    <Grid Margin="96,0,0,129">
        <DataGrid x:Name="KlienciTabela" Margin="-84,10,10,-10" Background="#a7a9b5"></DataGrid>
        <Button x:Name="Dodaj" Content="Dodaj" HorizontalAlignment="Left" Height="23" Margin="507,323,0,-200" VerticalAlignment="Top" Width="69" Click="Klienci_dodaj" Background="#a7a9b5" BorderBrush="#3d3e40" Style="{DynamicResource ButtonStyle}"/>
        <TextBox x:Name="ImieText" HorizontalAlignment="Left" Height="23" Margin="-72,323,0,-200" TextWrapping="Wrap" Text="Imie" VerticalAlignment="Top" Width="99" Background="#a7a9b5" BorderBrush="#3d3e40"/>
        <TextBox x:Name="NazwiskoText" HorizontalAlignment="Left" Height="23" Margin="40,323,0,-200" TextWrapping="Wrap" Text="Nazwisko" VerticalAlignment="Top" Width="99" Background="#a7a9b5" BorderBrush="#3d3e40"/>
        <TextBox x:Name="AdresText" HorizontalAlignment="Left" Height="23" Margin="155,323,0,-200" TextWrapping="Wrap" Text="Adres" VerticalAlignment="Top" Width="99" Background="#a7a9b5" BorderBrush="#3d3e40"/>
        <TextBox x:Name="TelefonText" HorizontalAlignment="Left" Height="23" Margin="270,323,0,-200" TextWrapping="Wrap" Text="Telefon" VerticalAlignment="Top" Width="99" Background="#a7a9b5" BorderBrush="#3d3e40"/>
        <TextBox x:Name="EmailText" HorizontalAlignment="Left" Height="23" Margin="387,323,0,-200" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="99" Background="#a7a9b5" BorderBrush="#3d3e40"/>
        <StackPanel HorizontalAlignment="Left" Height="292" Margin="-84,10,0,-200" VerticalAlignment="Top" Width="772"/>
        <StackPanel HorizontalAlignment="Left" Height="62" Margin="-84,307,0,-200" VerticalAlignment="Top" Width="673"/>
        <Button Content="Wróć" HorizontalAlignment="Left" Height="23" Margin="609,380,0,-200" VerticalAlignment="Top" Width="69" Click="main_click" Background="#a7a9b5" BorderBrush="#3d3e40" Style="{DynamicResource ButtonStyle}"/>
        <Button Content="Zapisz" HorizontalAlignment="Left" Height="23" Margin="609,351,0,-200" VerticalAlignment="Top" Width="69" Click="zapisz_click" Background="#a7a9b5" BorderBrush="#3d3e40" Style="{DynamicResource ButtonStyle}"/>
        <Button Content="Usuń" HorizontalAlignment="Left" Height="23" Margin="609,323,0,-200" VerticalAlignment="Top" Width="69" Click="usun_click" Background="#a7a9b5" BorderBrush="#3d3e40" Style="{DynamicResource ButtonStyle}"/>
    </Grid>
</Window>
